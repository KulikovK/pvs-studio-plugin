{
  "name": "pvs-studio-plugin",
  "displayName": "PVS-Studio-Plugin",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pvs-studio-plugin.OpenReportsFile"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pvs-studio-plugin.OpenReportsFile",
        "title": "Open reports file PVS-Studio (JSON)"
      }
    ],
    "viewsContainers": {
     "panel": [
       {
         "id": "PVSPanel",
         "title": "PVS Report",
         "icon": ""
       }
     ]
    },
    "views": {
      "PVSPanel" :[
        {
          "id": "PVSPanelView",
          "name": "PVS Panel View",
          "icon": "",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jquery": "^3.5.14",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.0.0",
    "@types/mui-datatables": "^3.7.8",
    "@types/node": "^14.18.12",
    "@types/react-dom": "^17.0.12",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "datatables.net": "^1.11.5",
    "datatables.net-dt": "^1.11.5",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "jquery": "^3.6.0",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.3",
    "node": "^17.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "class-transformer": "^0.5.1",
    "event-stream": "^4.0.1",
    "jsonfile": "^6.1.0",
    "JSONStream": "^1.3.5",
    "load-json-file": "^7.0.1",
    "mui-datatables": "^4.1.2",
    "node-module": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "stream-json": "^1.7.4",
    "ts-serializable": "^2.0.48",
    "tsc": "^2.0.4",
    "tss-react": "^3.4.2"
  }
}
